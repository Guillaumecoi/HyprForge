{
  "custom/gpuinfo": {
    "exec": "hyde-shell gpuinfo",
    "return-type": "json",
    "format": "{0}",
    "rotate": 0,
    "interval": 5,
    "tooltip": true,
    "max-length": 1000,
    "on-click": "hyde-shell gpuinfo --toggle",
    "menu": "on-click-right",
    "menu-file": "$HOME/.config/waybar/menus/gpuinfo.xml",
    "menu-actions": {
      "toggle": "hyde-shell gpuinfo --toggle",
      "reset": "hyde-shell gpuinfo --reset",
    },
  },
}
